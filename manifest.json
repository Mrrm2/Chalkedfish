{
	"manifest_version": 3,
	"name": "Chalkedfish",
	"version": "1.0",
	"description": "Game Review but chalked",

	"action": {
		"default_title": "Chalkedfish"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["tabs", "activeTab", "scripting"]
}
