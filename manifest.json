{
	"manifest_version": 3,
	"name": "Chalkedfish",
	"version": "1.0",
	"description": "Game Review but chalked",

	"action": {
		"default_title": "Chalkedfish"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["tabs", "activeTab", "scripting"],
	"icons": {
		"16": "./images/chalkedfish16.png",
		"24": "./images/chalkedfish24.png",
		"32": "./images/chalkedfish32.png",
		"48": "./images/chalkedfish48.png",
		"128": "./images/chalkedfish128.png"

	}
}
